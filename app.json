{
  "expo": {
    "name": "Game Deal Finder",
    "slug": "game-deal-finder",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/icon-android.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#212121"
    },
    "scheme": "gamedealfinder",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onowrouzi.gamedealfinder"
    },
    "android": {
      "package": "com.onowrouzi.gamedealfinder",
      "permissions": ["VIBRATE"],
      "versionCode": 8
    }
  }
}
